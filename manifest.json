{
  "manifest_version": 2,

  "name": "Bundles",
  "description": "Put away tabs and reduce clutter",
  "version": "0.1.1",

  "browser_action": {
    "default_icon": {
      "19": "assets/bundles.png"
    },
    "default_popup": "popup.html",
    "default_title": "Bundle up"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ]
}
